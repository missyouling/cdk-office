# CDK-Office Test Configuration

# Test environment variables
TEST_DATABASE_URL=postgresql://test:test@localhost:5432/cdk_office_test?sslmode=disable
TEST_REDIS_URL=redis://localhost:6379/1
TEST_DIFY_API_KEY=test_api_key
TEST_DIFY_BASE_URL=http://localhost:8000

# Test coverage threshold (in percentage)
COVERAGE_THRESHOLD=80

# Test timeout (in seconds)
TEST_TIMEOUT=300

# Parallel test execution
PARALLEL_TESTS=true
PARALLEL_COUNT=4

# Test output format
TEST_OUTPUT_FORMAT=verbose

# Test tags to exclude
EXCLUDE_TAGS=integration,slow

# Test tags to include
INCLUDE_TAGS=unit